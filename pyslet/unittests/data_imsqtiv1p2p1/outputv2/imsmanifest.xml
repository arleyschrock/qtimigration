<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns="http://www.imsglobal.org/xsd/imscp_v1p1"
	xmlns:imsmd="http://www.imsglobal.org/xsd/imsmd_v1p2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.imsglobal.org/xsd/imscp_v1p1 http://www.imsglobal.org/xsd/imscp_v1p1.xsd http://www.imsglobal.org/xsd/imsmd_v1p2 http://www.imsglobal.org/xsd/imsmd_v1p2p4.xsd http://www.imsglobal.org/xsd/imsqti_v2p1 http://www.imsglobal.org/xsd/imsqti_v2p1.xsd"
	xmlns:imsqti="http://www.imsglobal.org/xsd/imsqti_v2p1" identifier="outputv2">
	<metadata>
		<schema>IMS Content</schema>
		<schemaversion>1.2</schemaversion>
	</metadata>
	<organizations/>
	<resources>
		<resource identifier="emptyitem" type="imsqti_item_xmlv2p1" href="empty/emptyitem.xml">
			<metadata>
				<imsmd:lom>
					<imsmd:general>
						<imsmd:identifier>emptyitem</imsmd:identifier>
						<imsmd:description>
							<imsmd:langstring>Empty Item</imsmd:langstring>
						</imsmd:description>
					</imsmd:general>
				</imsmd:lom>
			</metadata>
			<file href="empty/emptyitem.xml"/>
		</resource>
		<resource identifier="fullitem" type="imsqti_item_xmlv2p1" href="full/fullitem.xml">
			<metadata>
				<imsmd:lom>
					<imsmd:general>
						<imsmd:identifier>fullitem</imsmd:identifier>
						<imsmd:description>
							<imsmd:langstring>Full Item</imsmd:langstring>
						</imsmd:description>
					</imsmd:general>
					<imsmd:educational>
						<imsmd:context>
							<imsmd:source>
								<imsmd:langstring xml:lang="x-none">LOMv1.0</imsmd:langstring>
							</imsmd:source>
							<imsmd:value>
								<imsmd:langstring xml:lang="x-none">training</imsmd:langstring>
							</imsmd:value>
						</imsmd:context>						
						<imsmd:description>
							<imsmd:langstring xml:lang="en-GB">Migration</imsmd:langstring>
						</imsmd:description>						
					</imsmd:educational>
					<imsmd:annotation>
						<imsmd:description>
							<imsmd:langstring>Warning: maxattempts can not be controlled at item level, ignored: maxattempts='1'
Note: in future, maxattempts will probably be controllable at assessment or assessment section level
Warning: duration is currently outside the scope of version 2: ignored 1H</imsmd:langstring>
						</imsmd:description>						
					</imsmd:annotation>
				</imsmd:lom>
			</metadata>
			<file href="full/fullitem.xml"/>
		</resource>
		<resource identifier="qmdextensions" type="imsqti_item_xmlv2p1" href="qmdextensions/qmdextensions.xml">
			<metadata>
				<imsmd:lom>
					<imsmd:general>
						<imsmd:identifier>qmdextensions</imsmd:identifier>
						<imsmd:keyword>
							<imsmd:langstring xml:lang="en-GB">QMD</imsmd:langstring>
						</imsmd:keyword>
						<imsmd:keyword>
							<imsmd:langstring xml:lang="en-GB">Extension</imsmd:langstring>
						</imsmd:keyword>						
						<imsmd:keyword>
							<imsmd:langstring xml:lang="en-GB">Extension Domain</imsmd:langstring>
						</imsmd:keyword>						
					</imsmd:general>
					<imsmd:annotation>
						<imsmd:description>
							<imsmd:langstring>Warning: qmd_domain extension field will be added as LOM keyword</imsmd:langstring>
						</imsmd:description>						
					</imsmd:annotation>
				</imsmd:lom>
			</metadata>
			<file href="qmdextensions/qmdextensions.xml"/>
		</resource>
	</resources>
